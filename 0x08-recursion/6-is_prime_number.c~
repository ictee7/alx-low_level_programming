#include "main.h"

/**
 *is_prime_number - Function that returns the power of a number
 *@n: parameter passed
 *Return: 1 or -1 or value x raised to the power y
 */
int is_prime_number(int n )
{

	if (n == 0)
		return (1);
	else if (n < 0)
		return (-1);
	else
		return (x * is_prime_number (x, y - 1));
}
